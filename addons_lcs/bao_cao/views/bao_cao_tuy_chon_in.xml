                            <field name="NGUOI_KY_CHUC_DANH_GIAM_DOC" />
                            <field name="NGUOI_KY_TIEU_DE_GIAM_DOC" />
                            <field name="NGUOI_KY_TEN_GIAM_DOC" />
                            
                            <field name="NGUOI_KY_CHUC_DANH_KE_TOAN_TRUONG" />
                            <field name="NGUOI_KY_TIEU_DE_TOAN_TRUONG" />
                            <field name="NGUOI_KY_TEN_KE_TOAN_TRUONG" />

                            <field name="NGUOI_KY_CHUC_DANH_THU_KHO" />
                            <field name="NGUOI_KY_TIEU_DE_THU_KHO" />
                            <field name="NGUOI_KY_TEN_THU_KHO" />

                            <field name="NGUOI_KY_CHUC_DANH_THU_QUY" />
                            <field name="NGUOI_KY_TIEU_DE_THU_QUY" />
                            <field name="NGUOI_KY_TEN_THU_QUY" />
                                
                            <field name="NGUOI_KY_CHUC_DANH_NGUOI_LAP_BIEU" />
                            <field name="NGUOI_KY_TIEU_DE_NGUOI_LAP_BIEU" />
                            <field name="NGUOI_KY_TEN_NGUOI_LAP_BIEU" />
                        </group>
                        <footer>
                            <button name="action_view_report" string="Đồng ý" type="object" class="oe_highlight" />
                            <button name="button_cancel" string="Hủy bỏ" type="object" special="cancel" />
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>