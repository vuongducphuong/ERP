            <field name="name">bao.cao.tuy.chon.in.form</field>
            <field name="model">bao.cao.tuy.chon.in</field>
            <field name="arch" type="xml">
                <form string="Tùy chỉnh in">
                    <sheet>
                        <field name="name" invisible="1"/>
