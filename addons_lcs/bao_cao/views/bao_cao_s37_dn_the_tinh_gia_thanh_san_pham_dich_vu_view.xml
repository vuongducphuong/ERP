        <record id="view_bao_cao_s37_dn_the_tinh_gia_thanh_san_pham_dich_vu_tham_so_form" model="ir.ui.view">
            <field name="name">bao.cao.s37.dn.the.tinh.gia.thanh.san.pham.dich.vu.form</field>
            <field name="model">bao.cao.s37.dn.the.tinh.gia.thanh.san.pham.dich.vu</field>
            <field name="arch" type="xml">
                <form string="Chọn tham số...">
                    <sheet>
                        <group col="4">
                            <field name="THONG_KE_THEO" colspan="4" />
                            <field name="CHI_NHANH_ID" options="{'showClearButton': false,'columns': ['MA_DON_VI', 'TEN_DON_VI'], 'strings': ['Mã đơn vị', 'Tên đơn vị'],'displays': ['TEN_DON_VI']}" colspan="2" />
                            <field name="BAO_GOM_SO_LIEU_CHI_NHANH_PHU_THUOC" colspan="2" />
                            <field name="LOAI_GIA_THANH" invisible="1" />
                        </group>
                        <group col="4">
                            <field name="KY_TINH_GIA_THANH" domain="[('LOAI_GIA_THANH', 'in', ('DON_GIAN','HE_SO_TY_LE','CONG_TRINH'))]" options="{'columns': ['TEN', 'TEN_LOAI_GIA_THANH'], 'strings': ['Tên kỳ tính giá thành', 'Loại'],'displays': ['TEN']}" colspan="4" />
                        </group>
                        <notebook>
                            <page string="Chi tiết" attrs="{'invisible': [('LOAI_GIA_THANH', '!=', 'DON_GIAN')]}">
                                <field name="DOI_TUONG_THCP_IDS_GIAN_DON" mode="tree">
                                    <tree report="true" create="false" edit="false" delete="false">
                                        <field name="MA_THANH_PHAM_ID" string="Mã thành phẩm" />
                                        <field name="TEN_THANH_PHAM" string="Tên thành phẩm" />
                                        <field name="MA_DOI_TUONG_THCP_ID" string="Mã đối tượng THCP" />
                                        <field name="TEN_DOI_TUONG_THCP" string="Tên đối tượng THCP" />
                                    </tree>
                                </field>
                            </page>
                            <page string="Chi tiết" attrs="{'invisible': [('LOAI_GIA_THANH', '!=', 'HE_SO_TY_LE')]}">
                                <field name="DOI_TUONG_THCP_IDS_HE_SO_TY_LE" mode="tree">
                                    <tree report="true" create="false" edit="false" delete="false">
                                        <field name="MA_THANH_PHAM_ID" string="Mã thành phẩm" />
                                        <field name="TEN_THANH_PHAM" string="Tên thành phẩm" />
                                        <field name="MA_DOI_TUONG_THCP_ID" string="Mã đối tượng THCP" />
                                        <field name="TEN_DOI_TUONG_THCP" string="Tên đối tượng THCP" />
                                    </tree>
                                </field>
                            </page>
                            <page string="Chi tiết" attrs="{'invisible': [('LOAI_GIA_THANH', '!=', 'CONG_TRINH')]}">
                                <field name="DOI_TUONG_THCP_IDS_CONG_TRINH" mode="tree">
                                    <tree report="true" create="false" edit="false" delete="false">
                                        <field name="MA_CONG_TRINH_ID" string="Mã công trình" />
                                        <field name="TEN_CONG_TRINH" string="Tên công trình" />
                                        <field name="LOAI_CONG_TRINH" string="Loại công trình" />
                                        <field name="BAC" string="Bậc" />
                                    </tree>
                                </field>
                            </page>
                            
                        </notebook>
                        <!-- Master/Detail form -->
                        <footer>
                            <button name="action_view_report" string="Đồng ý" type="object" class="oe_highlight" context="{'discard_changes': True}" />
                            <button name="button_cancel" string="Hủy bỏ" type="object" special="cancel" />
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>
